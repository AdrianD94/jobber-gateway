  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | pino-pretty -c",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && ts-node tools/createAssets.ts",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "start": "pm2 start ./build/src/app.js -i 5 --attach --watch | pino-pretty -c ",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adriand94/jobber-shared": "^0.0.6",
    "@elastic/elasticsearch": "^8.11.0",
    "axios": "^1.6.4",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "pino-pretty": "^10.3.1",
    "typescript": "^5.3.3",
    "typescript-transform-paths": "^3.4.6",
    "winston": "^3.11.0"
